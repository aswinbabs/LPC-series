Group {
    Name ="Vectors";
    CheckBox { Name = "Enable"; Index = 0x4000; }

    CheckBox { Name = "Reset";                      Index = 0x4010; Disable { Condition { Name = "Enable"; Value = 0; } } }
    CheckBox { Name = "Hard Fault";                 Index = 0x4020; Disable { Condition { Name = "Enable"; Value = 0; } } }
    CheckBox { Name = "Interrupt Error";            Index = 0x4030; Disable { Condition { Name = "Enable"; Value = 0; } } }
    CheckBox { Name = "Bus Fault";                  Index = 0x4040; Disable { Condition { Name = "Enable"; Value = 0; } } }
    CheckBox { Name = "Usage Fault State Error";    Index = 0x4050; Disable { Condition { Name = "Enable"; Value = 0; } } }
    CheckBox { Name = "Usage Fault Checking Error"; Index = 0x4060; Disable { Condition { Name = "Enable"; Value = 0; } } }
    CheckBox { Name = "Usage Fault No Coprocessor"; Index = 0x4070; Disable { Condition { Name = "Enable"; Value = 0; } } }
    CheckBox { Name = "Memory Management";          Index = 0x4080; Disable { Condition { Name = "Enable"; Value = 0; } } }
}

Group {
    Name ="ITM Events";

    CheckBox { Name = "Enable";  Index = 0x5000; }

    Text  { Name = "8-bit overflow counter events"; Option = 0x10; } // Option: 0x10 - left align display

    CheckBox { Name = "CPI";    Index = 0x5010; Disable { Condition { Name = "Enable"; Value = 0; } } } Text { Name = "(cycles per instruction)"; Option = 0x10; }
    CheckBox { Name = "LSU";    Index = 0x5011; Disable { Condition { Name = "Enable"; Value = 0; } } } Text { Name = "(load-store cycles)"; Option = 0x10; }
    CheckBox { Name = "FOLD";   Index = 0x5012; Disable { Condition { Name = "Enable"; Value = 0; } } } Text { Name = "(folded instructions)"; Option = 0x10; }
    CheckBox { Name = "EXC";    Index = 0x5013; Disable { Condition { Name = "Enable"; Value = 0; } } } Text { Name = "(exception overhead)"; Option = 0x10; }
    CheckBox { Name = "SLEEP";  Index = 0x5014; Disable { Condition { Name = "Enable"; Value = 0; } } } Text { Name = "(sleep overhead)"; Option = 0x10; }
    CheckBox { Name = "EXCTRC"; Index = 0x5015; Disable { Condition { Name = "Enable"; Value = 0; } } } Text { Name = "(exception tracing)"; Option = 0x10; }

    Text  { Name = "Scaled CPU cycle events"; Option = 0x10; } // Option: 0x10 - left align display

    CheckBox { Name = "CYC";
        Index = 0x5016; 
        Disable { Condition { Name = "Emit PC Sample"; Value = 1; } } 
        Disable { Condition { Name = "Enable"; Value = 0; } } 
    } 
    Text { Name = "(CPU clock cycles)"; Option = 0x10; }

    CheckBox { Name = "Emit PC Sample"; 
        Index = 0x5021; 
        Disable { Condition { Name = "CYC"; Value = 1; } } 
        Disable { Condition { Name = "Enable"; Value = 0; } } 
    }

    ComboBox {
        Name = "Every N Cycles";
        Value ="64,128,192,256,320,384,448,512,576,640,704,768,832,896,960,1024,2048,3072,4096,5120,6144,7168,8192,9216,10240,11264,12288,13312,14336,15360,16384";
        Default = 1024;
        Disable { 
            Condition { Name = "CYC"; Value = 0; } 
            Condition { Name = "Emit PC Sample"; Value = 0; } 
        } 
        Disable { Condition { Name = "Enable"; Value = 0; } }
        Index = 0x5022;
    }
}

Group {
    Name ="ITM Timestamp";

    CheckBox { Name = "Enable";   Index = 0x6000; }
    ComboBox {
