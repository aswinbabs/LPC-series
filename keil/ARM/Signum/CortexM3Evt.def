                    "Data Address Match=1"
                    ;
            Default = 1;
        }

        Disable { Condition { Name = "Output"; Value = 0; } }

        Index = 0x1120;
    }

    ComboBox {
        Name = "Access";

        ValueCond {
            Condition { Name = "Function"; Value = 2 3; }   // PC or clock cycle match
            Value = "Fetch=0";
            Default = 0;
        }
        ValueCond {
            Condition { Name = "Function"; Value = 4 1; }   // data value or address match
            Value = "Read=1,"
                    "Write=2,"
