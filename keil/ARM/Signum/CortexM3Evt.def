// CortexM3Evt.def - Definition of Cortex-M3 Hardware Events

Ident   = "DefEvt";
Name    = "CortexM3Evt";     // Must match with name of file
Version = "1.00";
Date    = "03/22/07";
// Date    = "1/8/2008";

Option {
    Name = "ChangeInRunAllowed";    // Allow change of EVT while CPU is running
}

Group {
    Name = "Comparator 1";

    ComboBox {
        Name = "Output";
        Value = "Disabled=0,"
                "ITM Trace=2,"
                "Watchpoint=3,"
                "ETM Event=4"
                ;
        Default = 0;
        Index = 0x1010;
    }

    ComboBox {
        Name = "Function";

        ValueCond {
            Condition { Name = "Output"; Value = 0; }   // Disabled=0
            Value = "None=0";
            Default = 0;
        }

        ValueCond {
            Condition { Name = "Output"; Value = 1; }   // Coupled=1
            Value = "Data Address Match=1";
            Default = 1;
        }

        ValueCond {
            Condition { Name = "Output"; Value = 2; }   // ITM=2
            Value = "Emit PC on Data Address Match=5,"
                    "Emit PC on Clock Cycle Count Match=6,"
                    "Emit Address Offset on Data Address Match=7,"
                    "Emit Data on Data Address Match=8,"
                    "Emit Data and Address Offset on Data Address Match=9,"
                    "Emit PC and Data on Data Address Match=10"
                    ;
            Default = 5;
        }

        ValueCond {
            Condition { Name = "Output"; Value = 3; }   // Watchpoint=3
            Value = "PC Match=2,"
                    "Data Address Match=1,"
                    "Data Value Match=4"
                    ;
            Default = 1;
        }

        ValueCond {
            Condition { Name = "Output"; Value = 4; }   // ETM=4
            Value = "PC Match=2,"
                    "Data Address Match=1,"
                    "Data Value Match=4"
                    ;
            Default = 1;
        }

        Disable { Condition { Name = "Output"; Value = 0; } }

        Index = 0x1110;
    }

    ComboBox {
        Name = "Access";

        ValueCond {
            Condition { Name = "Function"; Value = 2 3; }   // PC or clock cycle match
            Value = "Fetch=0";
            Default = 0;
        }
        ValueCond {
            Condition { Name = "Function"; Value = 4 1; }   // data value or address match
            Value = "Read=1,"
                    "Write=2,"
                    "Read or Write=3"
                    ;
            Default = 3;
        }
        ValueCond {
            Condition { Name = "Function"; }   // Any other (no value specified)
            Value = "Read or Write=3";
            Default = 3;
        }

        Disable { Condition { Name = "Output"; Value = 0; } }

        Index = 0x1210;
    }

    EditBox  {
        Name = "Value"; 
        Disable { Condition { Name = "Output"; Value = 0; } }
        Index = 0x1310;
    }

    EditBox  {
        Name = "Ignore Bits"; 
        Disable {
            Condition { Name = "Output"; Value = 0; }
        }
        Disable {
            Condition { Name = "Output";   Value = 2 3 4; }
            Condition { Name = "Function"; Value = 3 4 6; } // clock-cycle, data value
        }
        Index = 0x1410;
    }

    Text  {
        Name = "Data Address Range";
    }

    ComboBox  {
        Name = "Address Match 0"; 
        Value = "Comparator 0=0,None=1,Comparator 2=2,Comparator 3=3";
        Disable {
            Condition { Name = "Output"; Value = 0 2; }
        }
        Disable {
            Condition { Name = "Function"; Value = 1 2; } // Allowed for data value match only
        }
        Default = 1;
        Index = 0x1510;
    }

    ComboBox  {
        Name = "Address Match 1"; 
        Value = "Comparator 0=0,None=1,Comparator 2=2,Comparator 3=3";
        Disable {
            Condition { Name = "Output"; Value = 0 2; }
        }
        Disable {
            Condition { Name = "Function"; Value = 1 2; } // Allowed for data value match only
        }
        Default = 1;
        Index = 0x1610;
    }

    ComboBox {
        Name = "Data Value Size";
        Value = "Byte=0,HalfWord=1,Word=2";
        Disable {
            Condition { Name = "Output"; Value = 0 2; }
        }
        Disable {
            Condition { Name = "Function"; Value = 1 2; } // Allowed for data value match only
        }
        Default = 2;
        Index = 0x1710;
    }
}

Group {
    Name = "Comparator 0";

    ComboBox {
        Name = "Output";

        ValueCond {
            Condition { Name = "Comparator 1.Function"; Value = 4; }   // Function data value match
            Condition { Name = "Comparator 1.Address Match 0";     Value = 0; }   // Comparator 0 selected for addr match
            Value = "Coupled=1";
            Default = 1;
        }

        ValueCond {
            Condition { Name = "Comparator 1.Function"; Value = 4; }   // Function data value match
            Condition { Name = "Comparator 1.Address Match 1";       Value = 0; }   // Comparator 0 selected for addr match
            Value = "Coupled=1";
            Default = 1;
        }

        ValueCond {
            Condition { Name = "Comparator 1.Function";  }   // Any other function 
            Value = "Disabled=0,"
                    "ITM Trace=2,"
                    "Watchpoint=3,"
                    "ETM Event=4"
                    ;
            Default = 0;
        }

        Index = 0x1000;
    }

    ComboBox {
        Name = "Function";

        ValueCond {
            Condition { Name = "Output"; Value = 0; }   // Disabled=0
            Value = "None=0";
            Default = 0;
        }

        ValueCond {
            Condition { Name = "Output"; Value = 1; }   // Coupled=1
            Value = "Data Address Match=1";
            Default = 1;
        }

        ValueCond {
            Condition { Name = "Output"; Value = 2; }   // ITM=2
            Value = "Emit PC on Data Address Match=5,"
                    "Emit PC on Clock Cycle Count Match=6,"
                    "Emit Address Offset on Data Address Match=7,"
                    "Emit Data on Data Address Match=8,"
                    "Emit Data and Address Offset on Data Address Match=9,"
                    "Emit PC and Data on Data Address Match=10"
                    ;
            Default = 5;
        }

        ValueCond {
            Condition { Name = "Output"; Value = 3; }   // Watchpoint=3
            Value = "PC Match=2,"
                    "Clock Cycle Count Match=3,"
                    "Data Address Match=1"
                    ;
            Default = 1;
        }

        ValueCond {
            Condition { Name = "Output"; Value = 4; }   // ETM=4
            Value = "PC Match=2,"
                    "Clock Cycle Count Match=3,"
                    "Data Address Match=1"
                    ;
            Default = 1;
        }

        Disable { Condition { Name = "Output"; Value = 0; } }

        Index = 0x1100;
    }

    ComboBox {
        Name = "Access";

        ValueCond {
            Condition { Name = "Function"; Value = 2 3; }   // PC or clock cycle match
            Value = "Fetch=0";
            Default = 0;
        }
        ValueCond {
            Condition { Name = "Function"; Value = 4 1; }   // data value or address match
            Value = "Read=1,"
                    "Write=2,"
                    "Read or Write=3"
                    ;
            Default = 3;
        }
        ValueCond {
            Condition { Name = "Function"; }   // Any other (no value specified)
            Value = "Read or Write=3";
            Default = 3;
        }

        Disable { Condition { Name = "Output"; Value = 0 1; } }

        Index = 0x1200;
    }

    EditBox  {
        Name = "Value"; 
        Disable { Condition { Name = "Output"; Value = 0; } }
        Index = 0x1300;
    }

    EditBox  {
        Name = "Ignore Bits"; 
        Disable {
            Condition { Name = "Output"; Value = 0; }
        }
        Disable {
            Condition { Name = "Output";   Value = 2 3 4; }
            Condition { Name = "Function"; Value = 3 6; } // clock-cycle and data value match
        }
        Index = 0x1400;
    }
}

